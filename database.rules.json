{
  "rules": {
    "presence": {
      "$userId": {
        ".read": "auth != null",
        ".write": "auth != null && auth.uid == $userId"
      }
    },
    "workspaces": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$workspaceId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "videoCalls": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$chatId": {
        ".read": "auth != null",
        ".write": "auth != null",
        "signals": {
          ".read": "auth != null",
          ".write": "auth != null",
          "$signalId": {
            ".read": "auth != null",
            ".write": "auth != null"
          }
        },
        "participants": {
          ".read": "auth != null",
          ".write": "auth != null"
        },
        "status": {
          ".read": "auth != null",
          ".write": "auth != null"
        },
        "request": {
          ".read": "auth != null",
          ".write": "auth != null"
        },
        "response": {
          ".read": "auth != null",
          ".write": "auth != null"
        },
        "requests": {
          ".read": "auth != null",
          ".write": "auth != null",
          "$requestId": {
            ".read": "auth != null",
            ".write": "auth != null"
          }
        }
      }
    },
    ".read": false,
    ".write": false
  }
}