  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "build:fast": "NODE_OPTIONS=\"--max-old-space-size=8192\" SKIP_TYPE_CHECK=true next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint --max-warnings=100",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "emulators": "cd firebase_functions && npm run serve",
    "type-check": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsc --noEmit",
    "prepare": "husky",
    "gtm:upload": "cd google/gtm && node scripts/gtm-upload-fixed.js",
    "gtm:upload-dsgvo": "cd google/gtm && node scripts/gtm-upload-fixed.js configs/gtm-dsgvo-triggers-fixed.json",
    "gtm:upload-tracking": "cd google/gtm && node scripts/gtm-upload-fixed.js configs/gtm-erweiterte-tracking-konfiguration-fixed.json",
    "gtm:upload-subcategories": "cd google/gtm && node scripts/gtm-upload-fixed.js configs/gtm-all-subcategories-generated.json",
    "gtm:generate-subcategories": "cd google/gtm && node scripts/gtm-subcategory-generator.js",
    "gtm:clean-upload": "cd google/gtm && node scripts/gtm-clean-upload.js configs/gtm-complete-config.json",
    "gtm:debug": "cd google/gtm && node scripts/gtm-debug.js",
    "gtm:check-permissions": "cd google/gtm && node scripts/gtm-check-permissions.js",
    "gtm:validate": "node -e \"const config = require('./google/gtm/configs/gtm-dsgvo-triggers-fixed.json'); console.log('âœ… GTM Configuration is valid');\"",
    "export-live-data": "node scripts/export-live-data.js",
    "import-emulator-data": "node scripts/import-emulator-data.js",
    "setup-emulator": "npm run export-live-data && npm run import-emulator-data",
    "dev:emulator": "firebase emulators:start --import=./firebase-export-backup",
    "scan:console": "node scripts/scan-console-logs.js",
    "scan:console:src": "node scripts/scan-console-logs.js --dir=src",
    "scan:console:ci": "node scripts/scan-console-logs.js --json --ci --dir=src",
    "logs:remove": "node scripts/remove-console-logs.js --dir=src",
    "logs:remove:dry": "node scripts/remove-console-logs.js --dir=src --dry-run",
    "logs:remove:all": "node scripts/remove-console-logs.js --dir=src --include-api",
    "logs:remove:aggressive": "node scripts/remove-console-logs.js --dir=src --include-api --include-warn --include-error",
    "pdf:extract": "node scripts/extract-pdf-text.js"
  },
