<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 200" width="200" height="200">
  <defs>
    <linearGradient id="logoGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background Circle -->
  <circle cx="100" cy="100" r="90" fill="url(#logoGradient)" stroke="#ffffff" stroke-width="4"/>
  
  <!-- Building Icon -->
  <g fill="#ffffff">
    <!-- Main building -->
    <rect x="70" y="60" width="60" height="80" fill="#ffffff"/>
    
    <!-- Building details -->
    <rect x="75" y="70" width="8" height="8" fill="url(#logoGradient)"/>
    <rect x="87" y="70" width="8" height="8" fill="url(#logoGradient)"/>
    <rect x="99" y="70" width="8" height="8" fill="url(#logoGradient)"/>
    <rect x="111" y="70" width="8" height="8" fill="url(#logoGradient)"/>
    
    <rect x="75" y="85" width="8" height="8" fill="url(#logoGradient)"/>
    <rect x="87" y="85" width="8" height="8" fill="url(#logoGradient)"/>
    <rect x="99" y="85" width="8" height="8" fill="url(#logoGradient)"/>
    <rect x="111" y="85" width="8" height="8" fill="url(#logoGradient)"/>
    
    <rect x="75" y="100" width="8" height="8" fill="url(#logoGradient)"/>
    <rect x="87" y="100" width="8" height="8" fill="url(#logoGradient)"/>
    <rect x="99" y="100" width="8" height="8" fill="url(#logoGradient)"/>
    <rect x="111" y="100" width="8" height="8" fill="url(#logoGradient)"/>
    
    <!-- Door -->
    <rect x="90" y="115" width="20" height="25" fill="url(#logoGradient)"/>
  </g>
  
  <!-- Company Text -->
  <text x="100" y="170" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="12" font-weight="bold">COMPANY</text>
</svg>
